{
  "name": "screeps-typescript-starter",
  "version": "3.0.0",
  "description": "",
  "main": "dist/world_benchmark/main.js",
  "//": "If you add or change the names of destinations in screeps.json, make sure you update these scripts to reflect the changes",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "rollup -c --config-",
    "push-main": "rollup -c --environment DEST:main --config-",
    "push-pserver": "rollup -c --environment DEST:pserver --config-",
    "push-season": "rollup -c --environment DEST:season --config-",
    "push-sim": "rollup -c --environment DEST:sim --config-",
    "test": "npm run test-unit",
    "test-unit": "mocha test/unit/**/*.test.ts --verbose",
    "test-integration": "echo 'See docs/in-depth/testing.md for instructions on enabling integration tests'",
    "watch-main": "rollup -cw --environment DEST:main --config-",
    "watch-pserver": "rollup -cw --environment DEST:pserver --config-",
    "watch-season": "rollup -cw --environment DEST:season --config-",
    "watch-sim": "rollup -cw --environment DEST:sim --config-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screepers/screeps-typescript-starter.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/screepers/screeps-typescript-starter/issues"
  },
  "homepage": "https://github.com/screepers/screeps-typescript-starter#readme",
  "engines": {
    "node": "10.x || 16.x"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-generator-functions": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^15",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/chai": "^4.3.19",
    "@types/lodash": "^3.10.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.13.52",
    "@types/screeps": "^3.3.8",
    "@types/sinon": "^5.0.7",
    "@types/sinon-chai": "^3.2.12",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "chai": "^4.2.0",
    "lodash": "^3.10.1",
    "mocha": "^5.2.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.10",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-insert": "^1.3.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-nodent": "^0.2.2",
    "rollup-plugin-screeps": "^1.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "source-map": "~0.6.1"
  }
}
